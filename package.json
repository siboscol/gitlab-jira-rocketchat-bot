{
  "name": "gitlab-workflow-service",
  "version": "0.0.0",
  "description": "Service to automate Gitlab workflow and RocketChat group notification",
  "main": "src/index.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon src/index.js",
    "start": "node src/index.js",
    "lint": "eslint . && prettier --check \"**/*.{json,md,html,yaml,css}\"",
    "lint:fix": "eslint . --fix && prettier --write \"**/*.{json,md,html,yaml,css}\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.{json,md,html,yaml,css}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "@kazaar/express-error-handler": "^2.5.0",
    "axios": "^0.19.2",
    "body-parser": "^1.18.3",
    "dotenv": "^8.0.0",
    "express": "^4.16.4",
    "express-promise-router": "^3.0.3",
    "helmet": "^3.15.0",
    "http-errors": "^1.7.2",
    "moment": "^2.24.0",
    "morgan": "^1.9.1",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "cross-env": "^5.2.0",
    "eslint": "^5.13.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-promise": "^4.0.1",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.3",
    "nodemon": "^1.18.9",
    "prettier": "^2.0.5"
  },
  "engines": {
    "node": ">=10.16"
  }
}
